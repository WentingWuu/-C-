#include<bits/stdc++.h>
using namespace std;
int a[90]={};
void iscancel(int p,int k){
	if(p<k)
		cout<<"YES"<<endl;
	else
		cout<<"NO"<<endl;
}
int main()
{
	int n,m,p=0;
	cin>>n>>m;
	for(int i=0;i<n;i++){
		cin>>a[i];
	}
	for(int i=0;i<n;i++){
		if(a[i]<=0)
			p++;
	}
	iscancel(p,m);
	return 0;
}
